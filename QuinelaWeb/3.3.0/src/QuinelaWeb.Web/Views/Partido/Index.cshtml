@model IEnumerable<QuinelaWeb.Web.Models.Partidos.PartidoViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grupo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaisLocal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaisLocalGol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaisVisitante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PaisVisitanteGol)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grupo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaisLocal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaisLocalGol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaisVisitante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaisVisitanteGol)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.Id }) |
            @Html.ActionLink("Details", "Details", new {  id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new {  id=item.Id })
        </td>
    </tr>
}

</table>
